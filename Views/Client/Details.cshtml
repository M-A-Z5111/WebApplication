@model WebApplication.Models.Client

@Styles.Render("~/MyDesign/StyleSheetDetails.css")

<body onload="onload()">
    <div class="container">
        <h1>Client Info</h1>
        <div class="row">
            <div class="col-sm-5 col-xs-12">
                <form action="javascript:void(0);" autocomplete="off">

                    <div class="form-group">
                        <lable for="subject">Client Id：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.ClientId" readonly />
                    </div>
                    <div class="form-group">
                        <lable for="event">Client Name：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.ClientName" readonly />
                    </div>
                    <div class="form-group">
                        <lable for="event">Client Type：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.Classification" readonly />
                    </div>
                    <div class="form-group">
                        <lable for="event">Client Birthdate：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.Birthdate" readonly />
                    </div>
                    <div class="btn-group" role="group" aria-label="First group">
                        @* @Html.ActionLink("Back", "Index", new { @class = "btn btn-primary" })*@
                        <a class="btn btn-primary" href="/Client/Index">Back</a>
                    </div>
                </form>
            </div>

        </div>
    </div>
    <hr />

</body>