@model WebApplication.Models.Device

@Styles.Render("~/MyDesign/StyleSheetDetails.css")

<body onload="onload()">
    <div class="container">
        <h1>Device Info</h1>
        <div class="row">
            <div class="col-sm-5 col-xs-12">
                <form action="javascript:void(0);" autocomplete="off">

                    <div class="form-group">
                        <lable for="subject">Device Id：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.DeviceId" readonly/>
                    </div>
                    <div class="form-group">
                        <lable for="event">Device Name：</lable>
                        <input type="text" id="subject" class="form-control" value="@Model.DeviceName" readonly/>
                    </div>
                    <div class="btn-group" role="group" aria-label="First group">
                        @* @Html.ActionLink("Back", "Index", new { @class = "btn btn-primary" })*@
                        <a class="btn btn-primary" href="/Device/Index">Back</a>
                    </div>
                </form>
            </div>
            
        </div>
    </div>
    <hr />
    
</body>